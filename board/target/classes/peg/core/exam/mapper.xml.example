<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="${package}.service.${program}Mapper">
<!--
 *   프로그램명 : ${program}Mapper 데이터 처리 SQLMAP
 *   작  성  일 : xxxx.xx.xx
 *   작  성  자 : xxx(xxxx@pegsystem.co.kr)
 *   비      고 : 
 *	 Copyright (C) PEGSYSTEM <http://www.pegsystem.co.kr>
 -->
	
	
	<!--
	 작 성 일 : xxxx.xx.xx
	 작 성 자 : xxxx
	 내    용 : P2T_CD_D 테이블 조회
	 파라미터 : 코드이름(cd_nm, VARCHAR2)
	            사용여부(user_yn, VAHRCHAR2)
	-->
	<select id="selectCodeList" resultType="egovMap">
		<![CDATA[
		SELECT CD_ID
			  ,CD
			  ,CD_NM
			  ,LANG_SEQ
			  ,DSC
			  ,MGT_ITEM
			  ,RMK
			  ,USE_YN
			  ,INQ_ORD
			  ,ENT_UNO
			  ,ENT_DTM
			  ,UPD_UNO
			  ,UPD_DTM
		  FROM P2T_CD_D
		 WHERE CD_NM LIKE '%'||#{cd_nm}||'%'
		   AND USE_YN LIKE '%'||#{use_yn}||'%'
		]]> 
	</select>

</mapper>